<h1>Purchases</h1>

  <table class="table table-bordered">
    <thead>
      <tr>
        <th>id</th>
        <th>purchase_date</th>
        <th>quantity</th>
        <th>cost</th>
        <th>prod_id</th>
      </tr>
    </thead>

    <tbody>
    {{#purchases}}
        <tr>
          <td>{{id}}</td>
          <td>{{purchase_date}}</td>
          <td>{{quantity}}
          <td>{{cost}}</td>
          <td>{{prod_id}}</td>
        </tr>
    {{/purchases}}
  </tbody>
</table>
